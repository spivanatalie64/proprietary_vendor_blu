on post-fs-data
    chmod 0664 /dev/sostorch
    chown system system /dev/sostorch
	chown system system /sys/devices/platform/odm/odm:sostorch/level
	chown system system /sys/devices/platform/odm/odm:sostorch/mode
	chown system system /sys/devices/platform/odm/odm:sostorch/enable
	chmod 0664 /dev/hoperf
    chown system system /dev/hoperf
	chown system system /sys/devices/platform/11007000.i2c/i2c-0/0-0076/pressure
#aw22xxx
	chown system system /sys/class/leds/breath/brightness
	chown system system /sys/class/leds/breath/fw
	chown system system /sys/class/leds/breath/imax
	chown system system /sys/class/leds/breath/rgb
	chown system system /sys/class/leds/breath/task0
	chown system system /sys/class/leds/breath/cfg
	chown system system /sys/class/leds/breath/effect
	chown system system /sys/class/leds/breath/hwen
	chown system system /sys/class/leds/breath/max_brightness
    chown system system /sys/class/leds/breath/reg
    chown system system /sys/class/leds/breath/task1
	chmod 0664 /sys/class/leds/breath/brightness
	chmod 0664 /sys/class/leds/breath/fw
	chmod 0664 /sys/class/leds/breath/imax
	chmod 0664 /sys/class/leds/breath/rgb
	chmod 0664 /sys/class/leds/breath/task0
	chmod 0664 /sys/class/leds/breath/cfg
	chmod 0664 /sys/class/leds/breath/effect
	chmod 0664 /sys/class/leds/breath/hwen
	chmod 0664 /sys/class/leds/breath/max_brightness
	chmod 0664 /sys/class/leds/breath/reg
	chmod 0664 /sys/class/leds/breath/task1
#flashlight
	chown system camera /sys/devices/virtual/flashlight_core/flashlight/flashlight_capability
	chown system camera /sys/devices/virtual/flashlight_core/flashlight/flashlight_strobe
	chmod 0664 /sys/devices/virtual/flashlight_core/flashlight/flashlight_capability
	chmod 0664 /sys/devices/virtual/flashlight_core/flashlight/flashlight_strobe
#irtorch
	chmod 0666 /sys/class/flashlight_core/flashlight/flashlight_irtorch

#exergen
    chown system system /sys/devices/platform/exergen/open
    chmod 0666 /sys/devices/platform/exergen/open
    chown system system /sys/devices/platform/exergen/close
    chmod 0666 /sys/devices/platform/exergen/close
    chown system system /sys/kernel/prize/exergen/open
    chown system system /sys/kernel/prize/exergen/close
    chmod 0666 /sys/kernel/prize/exergen/open
    chmod 0666 /sys/kernel/prize/exergen/close
    chown system system /sys/kernel/fm78100/rawdata/ch0
    chown system system /sys/kernel/fm78100/rawdata/ch1
    chown system system /sys/kernel/fm78100/rawdata/ch2
    chmod 0666 /sys/kernel/fm78100/rawdata/ch0
    chmod 0666 /sys/kernel/fm78100/rawdata/ch1
    chmod 0666 /sys/kernel/fm78100/rawdata/ch2


#leds
    chmod 0666 /sys/class/leds/red/brightness
    chown system system /sys/class/leds/red/brightness
    chmod 0666 /sys/class/leds/green/brightness
    chown system system /sys/class/leds/green/brightness
    chmod 0666 /sys/class/leds/blue/brightness
    chown system system /sys/class/leds/blue/brightness
    chmod 0666 /sys/class/leds/red_1/brightness
    chown system system /sys/class/leds/red_1/brightness
    chmod 0666 /sys/class/leds/green_1/brightness
    chown system system /sys/class/leds/green_1/brightness
    chmod 0666 /sys/class/leds/blue_1/brightness
    chown system system /sys/class/leds/blue_1/brightness
#torch
	chown system system /sys/class/flashlight_core/flashlight/flashlight_torch
	chmod 0666 /sys/class/flashlight_core/flashlight/flashlight_torch
#charger_pe
	chown system system /sys/devices/platform/charger/Pump_Express
	chmod 0666 /sys/devices/platform/charger/Pump_Express
#flashlight
	chown system system /sys/class/flashlight_core/flashlight/flashlight_fled1
	chown system system /sys/class/flashlight_core/flashlight/flashlight_fled2
	chmod 0666  /sys/class/flashlight_core/flashlight/flashlight_fled1
	chmod 0666  /sys/class/flashlight_core/flashlight/flashlight_fled2
#tp glove mode
	chown system system /sys/kernel/prize/smartcover/common_node/state
	chmod 0666 /sys/kernel/prize/smartcover/common_node/state
#fm_ant
	chown system system /sys/devices/virtual/fm_ant/fm_ant_data/fm_ant_status
	chmod 0666 /sys/devices/virtual/fm_ant/fm_ant_data/fm_ant_status
# Sensor
    chmod 0666 /dev/hf_manager
